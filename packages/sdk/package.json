{
	"name": "@directus/sdk",
	"version": "9.0.0-rc.93",
	"description": "The official Directus SDK for use in JavaScript!",
	"repository": {
		"type": "git",
		"url": "https://github.com/directus/directus.git"
	},
	"author": "Rijk van Zanten <rijkvanzanten@me.com>",
	"license": "MIT",
	"keywords": [
		"api",
		"client",
		"cms",
		"directus",
		"headless",
		"javascript",
		"node",
		"sdk"
	],
	"main": "./dist/esm/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"exports": "./dist/esm/index.js",
	"files": [
		"dist"
	],
	"type": "module",
	"scripts": {
		"prebuild": "npm run cleanup",
		"build": "tsc --project ./tsconfig.json --module ES2015 --outDir ./dist/esm",
		"cleanup": "run-p cleanup:*",
		"cleanup:dist": "rimraf ./dist",
		"cleanup:coverage": "rimraf ./coverage",
		"test": "jest --coverage",
		"test:watch": "jest --coverage --watchAll",
		"dev": "npm run build -- -w --preserveWatchOutput --incremental"
	},
	"dependencies": {
		"axios": "^0.21.1"
	},
	"devDependencies": {
		"@types/jest": "27.0.2",
		"argon2": "0.28.2",
		"dotenv": "10.0.0",
		"jest": "27.2.1",
		"jest-environment-jsdom": "27.2.0",
		"nock": "13.1.3",
		"npm-run-all": "4.1.5",
		"rimraf": "3.0.2",
		"ts-jest": "27.0.5",
		"ts-node": "10.2.1",
		"typescript": "4.4.3"
	},
	"gitHead": "24621f3934dc77eb23441331040ed13c676ceffd"
}
